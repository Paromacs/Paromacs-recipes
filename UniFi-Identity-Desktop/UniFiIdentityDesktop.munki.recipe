<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>identifier</key>
    <string>com.github.paromacs.munki.unifi-identity-desktop.munki</string>
    <key>description</key>
    <string>Downloads a UniFi Identity Desktop disk image and imports into Munki.</string>
    <key>parent</key>
    <string>com.github.paromacs.download.unifi-identity-desktop</string>
    <key>input</key>
    <dict>
        <key>MUNKI_REPO_SUBDIR</key>
        <string>apps/ubiquiti</string>
        <key>NAME</key>
        <string>UniFiIdentityDesktop</string>
        <key>DISPLAY_NAME</key>
        <string>UniFi Identity Desktop</string>
        <key>DESCRIPTION</key>
        <string>The UniFi Identity Desktop client application for macOS.</string>
        <key>category</key>
        <string>Utilities</string>
        <key>developer</key>
        <string>Ubiquiti</string>
        <key>unattended_install</key>
        <true/>
    </dict>
    <key>process</key>
    <array>
        <dict>
            <key>processor</key>
            <string>MunkiImporter</string>
            <key>arguments</key>
            <dict>
                <key>pkg_path</key>
                <string>%pathname%</string>
                <key>repo_subdirectory</key>
                <string>%MUNKI_REPO_SUBDIR%</string>
                <key>name</key>
                <string>%NAME%</string>
                <key>display_name</key>
                <string>%DISPLAY_NAME%</string>
                <key>description</key>
                <string>%DESCRIPTION%</string>
            </dict>
        </dict>
    </array>
</dict>
</plist>
